{
    "name": "pikart/laravel-hooks",
    "description": "Simple hooks system for laravel",
    "keywords": ["laravel","hooks"],
    "license": "MIT",
    "authors": [
        {
            "name": "MichaÅ‚ Styrylski",
            "email": "styryl88@gmail.com"
        }
